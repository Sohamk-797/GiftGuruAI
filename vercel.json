{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(assets|favicon.ico|robots.txt|sitemap.xml|manifest.webmanifest)/?(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/$1/$2" },
    { "src": "/(.*)\\.(js|css|map|json|png|jpg|jpeg|svg|ico|webp|woff2?)$", "dest": "/$1" },

    { "src": "/google3f914dce4aa11c2d.html$", "dest": "/google3f914dce4aa11c2d.html" },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
